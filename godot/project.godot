[application]

config/name="DEATH AND TAXES"
config/description="DEATH AND TAXES game by Voice Box Games."
run/main_scene="res://intro/Intro.tscn"
boot_splash/image="res://splash.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.105882, 0.14902, 0.196078, 1 )
config/icon="res://icon.png"
